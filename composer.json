{
    "name": "serdaryigit/laravel-dhlpackage",
    "description": "DHL integration package with Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Serdar Yigit",
            "email": "serdaryigit1@gmx.de",
            "role": "Developer"
        }
    ],
    "keywords": [
        "laravel",
        "dhl",
        "parcel",
        "api"
    ],
    "homepage": "https://github.com/srygt/laravel-dhlpackage",    
    "require": {
        "php": "^7.3|^8.3",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^8.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Serdaryigit\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Serdaryigit\\Laravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Serdaryigit\\Laravel\\DhlServiceProvider"
            ],
            "aliases": {
                "Dhl": "Serdaryigit\\Laravel\\Facades\\Dhl"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}